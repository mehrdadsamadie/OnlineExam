@using Examination.WebUI.Models.Helper
@{
    ViewBag.Title = "Report";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .top-buffer {
        margin-top: 20px;
        margin-bottom: 80px;
    }
</style>
<h2>Report</h2>
<div class="col-lg-12">


    <div dir="rtl" class="col-md-2">
    </div>
    <div class="col-md-3" dir="rtl">
        <div class="row top-buffer">
            <span>Exam Result</span>  @Html.ImageActionLink(Url.Content("~/Image/excel-xls-icon.png"), "Report", "AdminResultExcel", "UserExam", null, null, new { title = "Roles", @class = "img-circle" })
        </div>
    </div>
    <div dir="rtl" class="col-md-2">
    </div>
    <div class="col-md-3" dir="rtl">
        <div class="row top-buffer">
            <span>Exam's Result</span>  @Html.ImageActionLink(Url.Content("~/Image/excel-xls-icon.png"), "Report", "AdminResultAllExcel", "UserExam", null, null, new { title = "Report", @class = "img-circle" })
        </div>
    </div>
    <div dir="rtl" class="col-md-2">
    </div>
    <div class="row top-buffer">
        <p></p>
    </div>

</div>
